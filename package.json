{
  "name": "supplysight-dashboard",
  "version": "1.0.0",
  "description": "Daily Inventory Dashboard for SupplySight",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["react", "tailwind", "graphql", "dashboard", "supply-chain"],
  "author": "SupplySight Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
